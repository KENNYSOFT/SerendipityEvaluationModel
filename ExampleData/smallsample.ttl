@prefix dbr: <http://dbpedia.org/resource/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix : <http://example.com/> .

########################
########1-Level#########
########################
:X
	:SubField :DomainA ;
	:SubField :DomainB .
	
:Y
	:SubField :DomainC .

########################
########2-Level#########
########################
:DomainA 
	:contains :NodeA1 ;
	:contains :NodeA2 ;
	:contains :NodeA3 ;
	:contains :NodeA4 .
	
:DomainB
	:contains :NodeB1 ;
	:contains :NodeB2 .
	
:DomainC :contains :NodeC1 .

########################
########3-Level#########
########################
:NodeA1 :related :NodeA4 .
:NodeA1 :related :NodeA3 .
:NodeA1 :related :NodeA2 .
:NodeA3 :related :NodeA4 .
:NodeA4 :related :NodeB1 .
:NodeA2 :related :NodeB2 .
:NodeA1 :related :NodeC1 .
:NodeA3 :related :NodeC1 .
:NodeA2 :related :NodeC1 .